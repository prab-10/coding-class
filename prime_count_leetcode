class Solution {
public:
    int countPrimes(int n) {
        
int count=1,flag=0;
          for(int i=3;i<=n;i++)
            {
              flag=0;
              for(int j=2;j<i;j++)
              {
                  if(i%j==0)
                      flag=1;
              }
               if(flag==0) 
                   count++;
                
            }
        return count;
        }

        
    
};
